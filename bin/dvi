#!/bin/sh

xrandr --output LVDS1 --off --output DP1 --off --output DP2 --off --output DP3 --off --output HDMI1 --off --output HDMI2 --mode 1440x900 --output HDMI3 --off --output VGA1 --off --output VIRTUAL1 --off
#feh --no-fehbg --bg-fill "$HOME/.background.jpg"
xset -dpms
nmcli radio wifi off
keyboard
pactl set-sink-volume @DEFAULT_SINK@ 80%
